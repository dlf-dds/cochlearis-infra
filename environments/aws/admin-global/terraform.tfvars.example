# Admin Global Environment Configuration
# This environment manages cross-account IAM users and groups
# Copy this file to terraform.tfvars and fill in your values

# =============================================================================
# Optional Variables (all have defaults)
# =============================================================================

# Project name - used for resource naming and tagging
# project = "cochlearis"

# Environment name
# environment = "global"

# AWS region for IAM resources (IAM is global but needs a region for API calls)
# region = "eu-central-1"

